{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "display": {
    "pipe": true,
    "showErrors": true,
    "noBuffer": true
  },
  "logo": null,
  "modules": [
    "os",
    "de",
    "wm",
    {
      "type": "gpu",
      "driverSpecific": true,
      "format": "{2}____{6}"
    },
    {
      "type": "terminal",
      "format": "{3}"
    },
    {
      "type": "shell",
      "format": "{3}"
    }
  ]
}